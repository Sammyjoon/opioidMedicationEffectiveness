    "    # except ValueError:\n",
    "    #     print(pageNumber)\n",
    "    #     break\n",
    "\n",
    "    # Take out the <div> of name and get its value\n",
    "    commentsBlock = soup.find_all('div', attrs={'class': 'ddc-comment'})\n",
    "\n",
    "    # print(commentsBlock)\n",
    "    for commentBlock in commentsBlock:\n",
    "        #print('--------------------------------------')\n",
    "        ##content > div.contentBox > div:nth-child(12) > div > div > p.ddc-text-size-small.user-name.user-type.user-type-2_non_member\n",
    "        commentHeaderBlock = commentBlock.find('div', attrs={'class': 'ddc-comment-header'})\n",
    "        #\n",
    "        #  Comment Header,  Name and Date\n",
    "        #\n",
    "        commentHeaders = commentHeaderBlock.find_all('span')\n",
    "        commentDuration = ''\n",
    "        for commentHeader in commentHeaders:\n",
    "            #\n",
    "            # Check if it is Date, year = 20XX\n",
    "            #\n",
    "            if ', 20' in commentHeader.text:\n",
    "                commentDate = commentHeader.text\n",
    "            else:\n",
    "                commentDate = ''\n",
    "            if 'Taken' in commentHeader.text:\n",
    "                commentDuration = commentHeader.text\n",
    "                if 'less than 1 month' in commentDuration:\n",
    "                    durations = 0\n",
    "                elif '1 to 6 months' in commentDuration:\n",
    "                    durations = 1\n",
    "                elif '6 months to 1 year' in commentDuration:\n",
    "                    durations = 2\n",
    "                elif '1 to 2 years' in commentDuration:\n",
    "                    durations = 3\n",
    "                elif '2 to 5 years' in commentDuration:\n",
    "                    durations = 4\n",
    "                elif '5 to 10 years' in commentDuration:\n",
    "                    durations = 5\n",
    "                elif '10 or more years' in commentDuration:\n",
    "                    durations = 6\n",
    "            elif not commentDuration:\n",
    "                commentDuration = ''\n",
    "        #\n",
    "        # \n",
    "        #\n",
    "        reviewBlock = commentHeaderBlock.find_next_sibling('p')\n",
    "        ConditionBlock = reviewBlock.find_all('b')\n",
    "        if len(ConditionBlock) > 0:\n",
    "            Condition = ConditionBlock[0].text\n",
    "            comment = reviewBlock.text.replace(Condition,'')\n",
    "        else:\n",
    "            comment = reviewBlock.text\n",
    "        \n",
    "        comment = comment.replace('“','')\n",
    "        comment = comment.replace('”','').replace('\\t','')\n",
    "        \n",
    "        RatingBlock = commentBlock.find('div',attrs={'class': 'ddc-rating-summary'})\n",
    "        if RatingBlock is not None:\n",
    "            RatingSpan = RatingBlock.find('span')\n",
    "            Rating = RatingSpan.find('b').text.strip()\n",
    "        else:\n",
    "            Rating = ''\n",
    "#         print(commentDuration)\n",
    "        if Rating:\n",
    "            newRating = (1+int(Rating))//2\n",
    "        else:\n",
    "            newRating =''\n",
    "            \n",
    "        row = pd.DataFrame({'comment': comment,\n",
    "                            'Effectiveness': newRating,\n",
    "                            'Duration of Treatment':durations,\n",
    "                            'Date':[commentDate]})\n",
    "\n",
    "        df = df.append(row, ignore_index=True)\n",
    "\n",
    "print(\"end\")\n",
    "\n"
   ]
